<div class="container">
    <img src="/public/assets/images/TennisMatchr.png" alt="tennisBanner" class="center">
    <nav class="navbar navbar-expand-lg navbar-light">
        <span class="navbar-brand" href="#">
            <img src="/public/assets/images/unnamed.jpg" width="30" height="30" alt="player">
        </span>
        <a id="navbar-brand" class="navbar-brand" href="/">Home</a>        
        <a id="navbar-brand" class="navbar-brand" href="/event">Events</a>       
        <a id="navbar-brand" class="navbar-brand" href="/player">Players</a>           
    </nav>
</div>
<h1>Players</h1>
<div class="container">
    <div class="row">
        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Email</th>
                    <th>Rating</th>
                </tr>
            </thead>
            <tbody>
                {{#each this}}
                <tr>
                    <th scope="row">{{id}}</th>
                    <td>{{first_name}}</td>
                    <td>{{last_name}}</td>
                    <td>{{email}}</td>
                    <td>{{rating}}</td>
                </tr>
                {{/each}}      
            </tbody>
        </table>
    </div>
</div>